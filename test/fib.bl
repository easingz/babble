function fib(n) {
	 fibs = {1, 1};
	 for i = 3, n
	     fibs[i] = fibs[i - 1] + fibs[i - 2]
	 return fibs[n]
}

println("fibonacci 10 is: ", fib(10))
println(fib(20))
println(fib(fib(5)))

function max_subarray(arr) {
	 max_end, max_so_far = 0
	 for x in array
	     max_end = max(0, max_end + x)
	     max_so_far = max(max_so_far, max_end)
	 return max_so_far
}
println("max of array {1, 2, -4, -1, 5, 3} is : " + max_subarray({1, 2, -4, -1, 5, 3}))